<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Run with ElasticSearch" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot">
    <module name="clugest.main" />
    <option name="SPRING_BOOT_MAIN_CLASS" value="com.cludus.clugest.ClugestApplication" />
    <option name="VM_PARAMETERS" value="-DALLOWED_ORIGIN_PATTERNS=http://localhost:[*] -DACTIVE_PROFILES=elasticsearch -DELASTICSEARCH_URIS=http://elasticsearch.clugest.home:9200" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.cludus.clugest.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>